{
  "configurations": {
    "Launch": {
      "adapter": "debugpy",
      "configuration": {
        "name": "Launch",
        "type": "python",
        "request": "launch",
        "cwd": "${workspaceRoot}",
        "stopOnEntry": true,
        "console": "externalTerminal",
        "debugOptions": [],
        "program": "${workspaceRoot}/${pyfile}"
      }
    }
  }
}
